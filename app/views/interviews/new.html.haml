= render 'layouts/header'
.interview_wrapper
  %h1 新規面接作成
  .interview_field
    %strong 開始時間
    %br
    =form_for [@user, @interview] do |f|
      =f.datetime_select :interview_date, {require: true, use_month_numbers: true}, class: 'interview_date'
      .block
        =f.submit '登録', class: "btn"
